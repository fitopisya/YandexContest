#include <iostream>
#include <string>
#include <vector>
#include <set>
#include <algorithm>

using namespace std;  

int GetPosition(char c){
   if (c >= 'a' && c <= 'z') {
      return c - 'a';
   }
   else if (c >= 'A' && c <= 'Z') {
      return c - 'A';
   }
   else  {
      return -1;
   }
}

string IntToHex(int a){
    string tmp("");
    do{
        int r(a%16);
        if (r>9) {
            r += (int)'A'-10;
        }
        else {
            r += (int)'0';
        }
        tmp = (char)r + tmp;
        a /= 16;
    } while (a);
    return tmp;
}
std::vector<std::string> SplitIntoWords(const std::string& text) {
    std::vector<std::string> words;
    std::string word;
    for (const char c : text) {
        if (c == ',') {
            if (!word.empty()) {
                words.push_back(word);
                word.clear();
            }
        } else {
            word += c;
        }
    }
    if (!word.empty()) {
        words.push_back(word);
    }
    return words;
}
struct Human {
    Human () = default;
    string surname, name, otch;
    int den = 0, mesic = 0, god = 0;
    
    int GetFirstStep() const {
        set<char> res;
        for (auto el : surname){
            res.insert(el);
        }
        for (auto el : name){
            res.insert(el);
        }
        for (auto el : otch){
            res.insert(el);
        }
        return res.size();
    }
    int GetSecondStep() const {
        int res = 0;
        int i1 = den, i2 = mesic;
        while (i1) {
            res += i1 % 10;
            i1 /= 10;
        }
        while (i2) {
            res += i2 % 10;
            i2 /= 10;
        }
        return res * 64;
    }
    int GetThirdStep() const {
        return (GetPosition(surname[0]) + 1) * 256;
    }
};

string GetFinalResult(string& str){
    if(str.size() > 3){
        reverse(str.begin(), str.end());
        str.resize(3);
        reverse(str.begin(), str.end());
    } return str;
}

int main()
{
    int count = 0;
    std::cin >> count;
    vector<Human> humanvec{};
    for (int i = 0; i < count; ++i){
        Human human;
        string tmp;
        cin >> tmp;
        std::vector<std::string> words = SplitIntoWords(tmp);
        human.surname = words[0];
        human.name = words[1];
        human.otch = words[2];
        human.den = stoi(words[3]);
        human.mesic = stoi(words[4]);
        human.god = stoi(words[5]);
        humanvec.push_back(human);
    }
    for (const auto el : humanvec){
        string res = (IntToHex(el.GetFirstStep() +  el.GetSecondStep() + el.GetThirdStep()));
        cout << GetFinalResult(res) << " ";
    }

}
